
var nextAction = "";
var nextTimeout = 0;

function navigateTo(screen, title, rights, timeout){
    if (isModalFrameVisible()) return;
    nextAction = 'navigateTo'+screen+'("'+title+'",'+rights+')';
    nextTimeout = timeout;
    top.getToolbarFrame().nextTimeout = timeout;

    navigateHandler("OK");
}

function navigateHandler(result){
    if (result != "OK") return;
    try{
        if(top.getUnitFrame() && typeof top.getUnitFrame().isOkToClose == 'function'){
            if (!top.getUnitFrame().isOkToClose('top.getToolbarFrame().'+nextAction)){
                return;
            }
        }
    } catch(e) {
        //Preventing error messages if unit frame is messed up because previous unit failed to load properly
    }
    doNextAction();
}

function doNextAction(){
    top.stopSystemTimeout();
    top.setFormSubmitted();
    try{
        eval(nextAction);
    } catch (e) {
        //This is to workaround IE bug when canceling onBeforeUnload confirmation dialog
    }
}

function isModalFrameVisible(){
    try{
        if(top.getUnitFrame() && top.getUnitFrame().document.getElementById('modalFrame')){
            if(top.getUnitFrame().document.getElementById('modalFrame').style.visibility == 'visible'){
                return true;
            }
        }
    } catch (e) {
        //Preventing error messages if unit frame is messed up because previous unit failed to load properly
    }
    return false;
}


function submitGlobalTopForm(){
    document.forms["globalTopForm"].elements['screenWidth'].value = top.getUnitFrameWidth();
    document.forms["globalTopForm"].elements['screenHeight'].value = top.getUnitFrameHeight();
    top.setFormSubmitted();
	if(!isNaN(nextTimeout) && nextTimeout > 0){
	    top.startSystemTimeout(nextTimeout);
	} else {
	    top.stopSystemTimeout();
	}
    document.forms["globalTopForm"].submit();
}

